<h1>Edit Band!</h1>

<form action="<%= band_url %>" method="post">
  <input type="hidden" name="_method" value="PATCH">

  <%= auth_token %>

  <label for="band_name">Name:</label>
  <input type="text" id="band_name" name="band[name]" value="<%= @band.name %>">
  <br>

  <input type="submit" value="Submit">
</form>
